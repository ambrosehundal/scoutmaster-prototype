<p id="notice"><%= notice %></p>

<h1>Awards And Honors</h1>

<table>
  <thead>
    <tr>
      <th>Award name</th>
      <th>Description</th>
      <th>Year</th>
      <th>Organization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @awards_and_honors.each do |awards_and_honor| %>
      <tr>
        <td><%= awards_and_honor.award_name %></td>
        <td><%= awards_and_honor.description %></td>
        <td><%= awards_and_honor.year %></td>
        <td><%= awards_and_honor.organization %></td>
        <td><%= link_to 'Show', awards_and_honor %></td>
        <td><%= link_to 'Edit', edit_awards_and_honor_path(awards_and_honor) %></td>
        <td><%= link_to 'Destroy', awards_and_honor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Awards And Honor', new_awards_and_honor_path %>
