.container-fluid
  .row.profile_head
    .col-md-3.profile-border
      %p
        Display Profile: #{@user.profile ?  "User has one profile" : "no user profile present!"}
      %h5
        =@profile.first_name
        =@profile.last_name
      %h5
        =@profile.age
        years old
      - if user_signed_in?
        %li You are currently logged in as
        =@user.email
        %li
        = link_to('Logout User', destroy_user_session_path, method: :delete)
      - else
        %li
        = link_to('Login', new_user_session_path)

    .col-md-8
      %br
      %br
      %h3 Headline
      =@user.profile.headline

      %br
      %br
      %h5 Summary
      =@user.profile.summary
      %br
      %br
      %br
      %br
      %li 
      = link_to('Create new award',  new_profile_awards_and_honors_path )
 

 
 
  
  










